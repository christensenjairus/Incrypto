{
  "name": "Incrypto",
  "productName": "Incrypto",
  "version": "4.5.0",
  "description": "Encrypted Messaging App",
  "engines": {
    "node": ">=15.0"
  },
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "Jairus Christensen",
  "license": "BYU",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "Incrypto",
        "icon": "./icons/hacker-25899.ico",
        "setupExe": "Incrypto_Setup-4.5.0.exe",
        "ignore": [
          "out",
          "dist"
        ]
      },
      "electronInstallerDebian": {
        "icon": "./icons/hacker-25899.png",
        "categories": [
          "Messaging"
        ],
        "ignore": [
          "out",
          "dist"
        ]
      },
      "electronInstallerRedhat": {
        "icon": "./icons/hacker-25899.png",
        "categories": [
          "Messaging"
        ],
        "ignore": [
          "out",
          "dist"
        ]
      },
      "github_repository": {
        "owner": "christensenjairus@gmail.com",
        "name": "Jairus Christensen"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "Incrypto"
      }
    }
  },
  "dependencies": {
    "axios": "^0.23.0",
    "bigint-mod-arith": "^3.0.2",
    "body-parser": "^1.19.1",
    "bootstrap-icons": "^1.7.1",
    "cookie-parser": "^1.4.6",
    "create-desktop-shortcuts": "^1.6.0",
    "crypto-js": "^4.1.1",
    "dompurify": "^2.3.3",
    "dotenv": "^14.2.0",
    "electron-alert": "^0.1.20",
    "electron-compile": "^6.4.4",
    "electron-prompt": "^1.7.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "encrypt-rsa": "^1.2.1",
    "express": "^4.17.2",
    "fs": "^0.0.1-security",
    "get-large-prime": "^1.0.3",
    "json-bigint": "^1.0.0",
    "mongodb": "^4.3.1",
    "multer": "^1.4.4",
    "open-in-editor": "^2.2.0",
    "openssl-nodejs": "^1.0.5",
    "perfect-scrollbar": "^1.5.3",
    "source-map-support": "^0.5.20",
    "svg.draggable.js": "^2.2.1",
    "svg.js": "^2.6.3",
    "typescript": "^4.5.0-dev.20211101"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-forge": "^5.2.4",
    "electron-prebuilt-compile": "8.2.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.27.1"
  },
  "build": {
    "asar": false,
    "appId": "Incrypto",
    "mac": {
      "category": "Messaging",
      "icon": "./icons/hacker-25899.icns",
      "files": [
        "!out/*",
        "!dist/*"
      ]
    },
    "linux": {
      "category": "Messaging",
      "icon": "./icons/hacker-25899.png",
      "files": [
        "!out/*",
        "!dist/*"
      ]
    }
  }
}
